    "url": "git+https://github.com/lazuee/react-router.git",
    "directory": "packages/react-router-hono"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.mjs",
    "./http": "./dist/hono/http.mjs",
