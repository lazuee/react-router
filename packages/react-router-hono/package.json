{
  "name": "@lazuee/react-router-hono",
  "type": "module",
  "version": "1.0.9",
  "description": "A vite plugin that integrates hono with react-router, providing presets to simplify production setups.",
  "keywords": [
    "hono",
    "react-router",
    "react-router-hono",
    "vercel"
  ],
  "license": "MIT",
  "author": {
    "name": "lazuee",
    "email": "lazuee.dev@gmail.com"
  },
  "homepage": "https://github.com/lazuee/react-router/tree/main/packages/react-router-hono#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lazuee/react-router.git",
    "directory": "packages/react-router-hono"
  },
  "exports": {
    ".": {
      "types": "./dist/hono.d.ts",
      "import": "./dist/hono.js"
    },
    "./cache": {
      "types": "./dist/cache.d.ts",
      "import": "./dist/cache.js"
    },
    "./http": {
      "types": "./dist/http.d.ts",
      "import": "./dist/http.js"
    }
  },
  "files": [
    "LICENSE.md",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "build": "rimraf dist && tsup"
  },
  "peerDependencies": {
    "@hono/node-server": "1.13.7",
    "@react-router/dev": "pre",
    "esbuild": "0.24.2",
    "hono": "4.6.15",
    "react-router": "pre",
    "vite": "5.4.11"
  },
  "dependencies": {
    "@vercel/nft": "0.29.0",
    "minimatch": "10.0.1",
    "monorepo-root": "3.3.1",
    "tinyexec": "0.3.2"
  },
  "devDependencies": {
    "@types/node": "22.10.5",
    "rimraf": "6.0.1",
    "terser": "5.37.0",
    "tsup": "8.3.5"
  }
}
