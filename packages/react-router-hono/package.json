  "sideEffects": false,
  "scripts": {
    "build": "tsdown"
  },
  "peerDependencies": {
    "@hono/node-server": "^1.14.3",
    "hono": "^4.7.11",
    "react-router": "^7.6.2",
    "vite": "^5.4.19 || ^6.3.5"
  },
  "dependencies": {
    "@vercel/nft": "^0.29.4",
    "minimatch": "^10.0.1",
    "resolve-workspace-root": "^2.0.0",
