{
  "name": "@lazuee/react-router-hono",
  "type": "module",
  "version": "1.0.9",
  "description": "A vite plugin that integrates hono with react-router, providing presets to simplify production setups.",
  "keywords": [
    "hono",
    "react-router",
    "react-router-hono",
    "vercel"
  ],
  "license": "MIT",
  "author": {
    "name": "lazuee",
    "email": "lazuee.dev@gmail.com"
  },
  "homepage": "https://github.com/lazuee/react-router/tree/main/packages/react-router-hono#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lazuee/react-router.git",
    "directory": "packages/react-router-hono"
  },
  "exports": {
    ".": {
      "types": "./dist/hono.d.ts",
      "import": "./dist/hono.js"
    },
    "./cache": {
      "types": "./dist/cache.d.ts",
      "import": "./dist/cache.js"
    },
    "./http": {
      "types": "./dist/http.d.ts",
      "import": "./dist/http.js"
    },
    "./serveStatic": {
      "types": "./dist/serveStatic.d.ts",
      "import": "./dist/serveStatic.js"
    }
  },
  "files": [
    "LICENSE.md",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "build": "pnpm run clean && tsup",
    "clean": "node -e \"import { rmSync } from 'node:fs'; rmSync('dist', { recursive: true, force: true });\""
  },
  "peerDependencies": {
    "@hono/node-server": "^1.13.7",
    "@react-router/dev": "^7.1.1",
    "esbuild": "^0.24.2",
    "hono": "^4.6.15",
    "react-router": "^7.1.1",
    "vite": "^5.1.8 || ^6.0.7"
  },
  "dependencies": {
    "@vercel/nft": "^0.29.0",
    "minimatch": "^10.0.1",
    "monorepo-root": "^3.3.1",
    "tinyexec": "^0.3.2"
  },
  "devDependencies": {
    "@types/bun": "^1.1.16",
    "@types/node": "^22.10.5",
    "terser": "^5.37.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3"
  }
}
