    "react-router",
    "react-router-hono",
    "vercel"
  ],
  "license": "MIT",
  "author": {
    "name": "lazuee",
    "email": "lazuee.dev@gmail.com"
  },
  "homepage": "https://github.com/lazuee/react-router/tree/main/packages/react-router-hono#readme",
  "repository": {
